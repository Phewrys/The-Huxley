import java.io.*;
import java.util.*;

public class HuxleyCode {
  public static void main(String args[]) {
    Scanner input = new Scanner(System.in);

    int numeros = input.nextInt();
    int maior = 0;

    while(numeros != 0){      
      if (numeros > maior){
        maior = numeros;
      }
      numeros = input.nextInt();
    }
    System.out.printf("%d", maior);
  }
}
