import java.io.*;
import java.util.*;

public class HuxleyCode {
  public static void main(String args[]) {
    Scanner input = new Scanner(System.in);

    double x = input.nextDouble();
    double y = input.nextDouble();

    if(x > y)
      System.out.printf("%.2f", x*2);
    else
    System.out.printf("%.2f", y*2);
  }
}
