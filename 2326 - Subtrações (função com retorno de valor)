import java.io.*;
import java.util.*;

public class HuxleyCode {
  public static void main(String args[]) {
    Scanner input = new Scanner(System.in);

    int x = input.nextInt();
    int y = input.nextInt();

    System.out.printf("%d%n", sub(x,y));   

  }
  public static int sub(int x, int y){
    return x/y;
  }
}
