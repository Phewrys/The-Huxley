import java.io.*;
import java.util.*;

public class HuxleyCode {
  public static void main(String args[]) {
    Scanner input = new Scanner(System.in);
    
    int soma = 0;	
	int n = input.nextInt();
	
	for(int i = 1;i <= n; i++){
	    int numero = input.nextInt();
		soma += numero;
	}

    System.out.printf("%d",soma);
  }
}
