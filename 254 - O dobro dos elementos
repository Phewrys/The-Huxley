import java.io.*;
import java.util.*;

public class HuxleyCode {
  public static void main(String args[]) {
    Scanner input = new Scanner(System.in);

    int x = input.nextInt();
    int vetor[] = new int[x];

    for(int i = 0; i < x; i++){
      vetor[i] = input.nextInt();
    }

    for(int j = 0; j < x; j++){
      System.out.printf("%d%n", vetor[j]*2);
    }
  }
}
