import java.io.*;
import java.util.*;

public class HuxleyCode {
  public static void main(String args[]) {
    Scanner input = new Scanner(System.in);

    int n = input.nextInt();
    int soma = 0;

    for(int i = 1; i <= n; i++){
      int valor = input.nextInt();
      soma += valor;
    }

    System.out.printf("%d", soma);
  }
}
