import java.io.*;
import java.util.*;

public class HuxleyCode {
  public static void main(String args[]) {
    Scanner input = new Scanner(System.in);
 
    double raio, area, pi = 3.14159;
    raio = input.nextDouble();
    area = pi*(raio*raio);
    System.out.printf("Area = %.4f%n", area/10000);
  }
}
